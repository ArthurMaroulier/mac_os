#! /usr/bin/env bash

# DESCRIPTION
# Installs NodeJs npm modules.

# SETTINGS
set -o nounset
set -o errexit
set -o pipefail
IFS=$'\n\t'
SCRIPT_PATH="$MAC_OS_CONFIG_PATH/bin/install_npm_modules"

# EXECUTION
if [[ -x "$SCRIPT_PATH" ]]; then
  "$SCRIPT_PATH"
else
  printf "WARNING: npm modules install script does not exist or is not executable.\n"
fi
