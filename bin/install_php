#! /usr/bin/env bash

# DESCRIPTION
# Installs PHP software.

# SETTINGS
set -o nounset
set -o errexit
set -o pipefail
IFS=$'\n\t'
SCRIPT_PATH="$MAC_OS_CONFIG_PATH/bin/install_php"

# EXECUTION
if [[ -x "$SCRIPT_PATH" ]]; then
  "$SCRIPT_PATH"
else
  printf "WARNING: PHP install script does not exist or is not executable.\n"
fi
