#! /usr/bin/env bash

# DESCRIPTION
# Installs applications with cask.

# SETTINGS
set -o nounset
set -o errexit
set -o pipefail
IFS=$'\n\t'
SCRIPT_PATH="$MAC_OS_CONFIG_PATH/bin/install_cask"

# EXECUTION
if [[ -x "$SCRIPT_PATH" ]]; then
  "$SCRIPT_PATH"
else
  printf "WARNING: Applications install with cask script does not exist or is not executable.\n"
fi
